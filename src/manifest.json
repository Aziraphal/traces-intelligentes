{
    "manifest_version": 3,
    "name": "Traces Intelligentes",
    "version": "1.0.0",
    "description": "Extension pour protéger votre vie privée.",
    "permissions": [
      "storage",
      "declarativeNetRequest",
      "browsingData",
      "tabs",
      "webNavigation",
      "notifications"
    ],
    "background": {
      "service_worker": "background/secure-background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }